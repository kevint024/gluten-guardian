{
  "name": "gluten-guardian",
  "version": "1.0.0",
  "description": "A React Native Expo app that helps users with gluten allergies scan food products and check for gluten ingredients",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "preview:web": "expo export --platform web && npx serve dist",
    "demo": "node demo.js",
    "switch": "node switch-version.js",
    "use-main": "node switch-version.js main",
    "use-camera": "node switch-version.js camera",
    "use-simple": "node switch-version.js simple"
  },
  "keywords": [
    "gluten-free",
    "food-scanner",
    "barcode-scanner",
    "react-native",
    "expo",
    "mobile-app",
    "allergen-detection"
  ],
  "author": "Gluten Guardian Team",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "expo": "~53.0.20",
    "expo-camera": "~16.1.11",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "~0.21.0",
    "@expo/metro-runtime": "~5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
